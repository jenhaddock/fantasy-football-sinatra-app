<h1>Fantasy Teams</h1>
<ul>
  <%Team.all.each do |t|%>
     <%@user = User.find_by(:id => t.user_id)%>
     <li><a href="/users/<%=@user.slug%>"><%=@user.slug%></a> - <a href="/teams/<%=t.slug%>"><%=t.name %></a></li>
  <%end%>
</ul>

